{
  "name": "mi-note-vue",
  "displayName": "MI NOTE",
  "version": "1.2.0",
  "private": true,
  "description": "A sample Framework7-Vue application Cordova application that responds to the deviceready event.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf node_modules/ plugins/ platforms/ package-lock.json yarn.lock",
    "ionic:serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "c:r:a": "yarn build && cordova run android",
    "res": "ionic cordova resources"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh ./scripts/pre-commit.sh",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cordova-android": "^8.0.0",
    "cordova-ios": "^5.0.1",
    "cordova-plugin-camera": "^4.1.0",
    "cordova-plugin-splashscreen": "^5.0.3",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-plugin-whitelist": "^1.3.4",
    "core-js": "2.6.9",
    "framework7": "^4.5.0",
    "framework7-icons": "^2.3.1",
    "framework7-vue": "^4.5.0",
    "moment": "^2.29.4",
    "register-service-worker": "^1.6.2",
    "tiptap": "^1.25.0",
    "tiptap-extensions": "^1.27.0",
    "vue": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-angular": "^8.1.0",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-pwa": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "cordova-plugin-device": "2.0.2",
    "eslint": "^6.3.0",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^3.0.5",
    "ionic": "^5.2.7",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "keywords": [
    "cordova",
    "vue",
    "framework7"
  ],
  "author": "NieLamu",
  "license": "MIT",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-camera": {
        "ANDROID_SUPPORT_V4_VERSION": "27.+"
      },
      "cordova-plugin-device": {}
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
}
